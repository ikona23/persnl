        background-image: -moz-linear-gradient(top, #3F5666,#2F3440);
        background-image: -ms-linear-gradient(top,#3F5666,#2F3440);
        background-image: -o-linear-gradient(top, #3F5666,#2F3440);
        background-image: linear-gradient(top, #3F5666,#2F3440)
    }

    .visual {
        width:60%;
        -moz-animation:visual 2s ease-out;
        -webkit-animation:visual 2s ease-out;
        background-color: #620f47;
        background-image: -webkit-gradient(linear, left top, left bottom, from(#620f47), to(#620f47));
        background-image: -webkit-linear-gradient(top, #660f4b, #620f47);
        background-image: -moz-linear-gradient(top, #660f4b, #620f47);
        background-image: -ms-linear-gradient(top, #620f47, #620f47);
        background-image: -o-linear-gradient(top, #660f4b,#2F3440);
        background-image: linear-gradient(top, #660f4b,#2F3440)
    }

