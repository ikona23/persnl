    @-moz-keyframes html-css { 0%  { width:0px;} 100%{ width:70%;}  }
    @-moz-keyframes jquery { 0%  { width:0px;} 100%{ width:65%;}  }
    @-moz-keyframes wordpress { 0%  { width:0px;} 100%{ width:80%;}  }
    @-moz-keyframes ror  { 0% { width:0px;} 100%{ width:65%;}  }
    @-moz-keyframes sql { 0% { width:0px;} 100%{ width:60%;}  }
    @-moz-keyframes visual { 0% { width:0px;} 100%{ width:60%;}  }

    @-webkit-keyframes graphic-design  { 0%  { width:0px;} 100%{ width:80%;}  }
    @-webkit-keyframes html-css  { 0%  { width:0px;} 100%{ width:70%;}  }
    @-webkit-keyframes jquery { 0%  { width:0px;} 100%{ width:65%;}  }
    @-webkit-keyframes wordpress { 0%  { width:0px;} 100%{ width:80%;}  }
    @-webkit-keyframes ror  { 0%  { width:0px;} 100%{ width:65%;}  }
    @-webkit-keyframes sql  { 0%  { width:0px;} 100%{ width:60%;}  }
